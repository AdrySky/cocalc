  "scripts": {
    "lint": "npx eslint --ignore-pattern '**/node_modules/**' '**/*.ts?(x)'",
    "test": "export SMC_TEST=true&& cd smc-util && npm test && cd ../smc-util-node && npm test && cd ../smc-hub && npm test && cd ../smc-webapp && npm test && cd ../smc-project && npm test",
    "coverage": "cd smc-util && npm run coverage && cd ../smc-util-node && npm run coverage && cd ../smc-hub && npm run coverage && cd ../smc-webapp && npm run coverage",
    "webpack-watch": "cd $SALVUS_ROOT; scripts/check_npm_packages.py; node scripts/update-color-scheme.js; webapp-lib/primus/update_primus; unset CC_STATICPAGES; CC_NOCLEAN=true SOURCE_MAP=true NODE_ENV=development node --max_old_space_size=8192 node_modules/webpack/bin/webpack.js --progress --watch",
    "webpack-debug": "cd $SALVUS_ROOT; node scripts/update-color-scheme.js; webapp-lib/primus/update_primus; SOURCE_MAP=true NODE_ENV=development webpack --progress",
    "webpack-production": "cd $SALVUS_ROOT;node  scripts/update-color-scheme.js; webapp-lib/primus/update_primus;  BABEL_ENV=production NODE_ENV=production node --max_old_space_size=8192 node_modules/webpack/bin/webpack.js --progress",
    "webpack-clean": "rm -rvf $SALVUS_ROOT/static/",
    "webpack-static": "cd $SALVUS_ROOT; node scripts/update-color-scheme.js; scripts/update_react_static && NODE_OPTIONS='--max-old-space-size=3000' CC_STATICPAGES=true NODE_ENV=development webpack --progress",
    "install-all": "scripts/smc-install-all",
    "make": "npm run install-all",
    "delete-package-lock": "git ls-files '../*/package-lock.json' | xargs rm -f",
    "restore-package-lock": "git ls-files '../*/package-lock.json' | xargs git checkout --",
    "clean": "find $SMC_ROOT -type d -name node_modules | xargs rm -rf; rm -rf $SMC_ROOT/static; npm cache verify",
    "audit-fix": "scripts/npm-audit.sh fix",
    "prettier-all": "git ls-files '*.ts' '*.tsx' '**/*.ts' '**/*.tsx' | parallel -j5 --eta prettier --write :::"
  },
  "repository": {
    "type": "git",
